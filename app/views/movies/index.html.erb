<h1>HomePage!</h1>

<% @movie.each do |movie| %>
    <p>
      <%= movie.title %>
      <%= link_to "Show", movie %>
      <%= link_to "Edit", edit_movie_path(movie) %>
      <%= link_to "Delete", movie_path(movie), method: :delete,
                  data: {confirm: "Are you sure?"}  %>
    </p>
<% end %>
<%= link_to "New Movie", new_movie_path %>